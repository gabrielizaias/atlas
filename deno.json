{
	"name": "@atlasland/atlas",
	"version": "0.5.2",
	"exports": {
		"./console": "./console/mod.ts",
		"./error": "./error/mod.ts",
		"./http": "./http/mod.ts"
	},
	"compilerOptions": {
		"allowJs": false,
		"lib": [
			"dom",
			"dom.iterable",
			"dom.asynciterable",
			"deno.ns"
		],
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"useUnknownInCatchVariables": true
	},
	"fmt": {
		"exclude": [
			".git",
			".deno",
			"cov"
		],
		"useTabs": true,
		"lineWidth": 100
	},
	"lint": {
		"exclude": [
			".git",
			".deno",
			"cov"
		]
	},
	"tasks": {
		"cache": "deno cache -r cli.ts console/mod.ts error/mod.ts http/mod.ts",
		"check": "deno check -r cli.ts console/mod.ts error/mod.ts http/mod.ts",
		"dev:website": "deno run -A --watch website/main.ts",
		"test": "deno test -A --no-check --coverage=./cov"
	},
	"imports": {
		"@std/assert": "jsr:@std/assert@0.212.0",
		"@std/http": "jsr:@std/http@0.212.0",
		"@std/fs": "jsr:@std/fs@0.212.0",
		"@std/path": "jsr:@std/path@0.212.0",
		"@std/cli": "jsr:@std/cli@0.212.0",
		"@std/log": "jsr:@std/log@0.212.0"
	}
}
